FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 1e-005;


boundaryField
{

    ".*OUTLET.*"
    {
        type            inletOutlet;
        value           uniform 0.01;
        inletValue      uniform 0.01;
    }


    ".*INLET.*"
    {
        type            compressible::turbulentMixingLengthDissipationRateInlet;
        value           uniform 0.01;
        mixingLength    0.014;
    }
	

    ".*(FLOOR|CEILING|BACK|SIDE|manikin).*"
    {
        type            compressible::epsilonWallFunction;
        value           uniform 0.01;
    }


}



