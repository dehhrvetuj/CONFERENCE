FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.001;


boundaryField
{

    ".*OUTLET.*"
    {
        type            inletOutlet;
        value           uniform 0.01;
        inletValue      uniform 0.01;
    }


    ".*INLET.*"
    {
        type            turbulentIntensityKineticEnergyInlet;
        value           uniform 0.01;
        intensity       0.1;
    }


    ".*(FLOOR|CEILING|BACK|SIDE|manikin).*"
    {
        type            compressible::kqRWallFunction;
        value           uniform 0;
    }

}



