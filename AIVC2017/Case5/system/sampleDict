					(	0	0.19	1.1	)
					(	0	0.19	1.3	)

					
					
					(	0.5	1.22	0.1	)
					(	0.5	1.22	0.6	)
					(	0.5	1.22	1.1	)
					(	0.5	1.22	1.5	)
				
					
					
					(	0	2.19	0.275	)
					(	0	2.19	0.55	)
					(	0	2.19	0.875	)
					(	0	2.19	1.1	)
					(	0	2.19	1.3	)
					
					
					(	0.35	2.19	0.2	)
					(	0.35	2.19	0.4	)
					(	0.35	2.19	0.6	)
					(	0.35	2.19	0.8	)
					(	0.35	2.19	1	)
					(	0.35	2.19	1.2	)
					(	0.35	2.19	1.4	)
					(	0.35	2.19	1.6	)
					(	0.35	2.19	1.8	)
					(	0.35	2.19	2	)
					(	0.35	2.19	2.2	)
					(	0.35	2.19	2.4	)
					
					
					
				);
    }
);

